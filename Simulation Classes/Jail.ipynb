    "class Jail:\n",
    "    def __init__(self, loc):\n",
    "        self.type = \"Jail\"\n",
    "        self.loc = loc\n",
    "\n",
    "    def __repr__(self):\n",
    "        return f'Jail is located at position {self.loc} on the Monopoly board.'\n",
    "    \n",
    "    def __lt__(self, other):\n",
    "        return self.loc < other.loc"
